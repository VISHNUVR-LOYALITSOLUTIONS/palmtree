<odoo>
  <data>
    <record model="ir.ui.view" id="sms_template_form_view">
            <field name="name">branch.policies</field>
            <field name="model">operating.unit</field>
            <field name="inherit_id" ref="operating_unit.view_operating_unit_form"/>
            <field name="arch" type="xml">

        <xpath expr="//field[@name='partner_id']" position="after">

             <group string="Allowed SMS Templates">
                    <field name="sms_template_id" widget="many2many_tags"/>
                </group>

        </xpath>
         </field>
        </record>

  </data>
</odoo>